{
  "name": "express-minimal-ts",
  "version": "1.0.0",
  "description": "express minimal template with typescript",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "tsc & node .",
    "dev": "nodemon",
    "test": "jest --watchAll",
    
    "typeorm:migration:generate": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:generate -n",
    "typeorm:migration:run": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:run",
    "typeorm:migration:revert": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:revert"
    
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/js-code-ua/express-minimal-ts.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/js-code-ua/express-minimal-ts/issues"
  },
  "homepage": "https://github.com/js-code-ua/express-minimal-ts#readme",
  "dependencies": {
    "@types/redis": "^4.0.11",
    "dotenv": "^10.0.0",
    "express": "^4.17.2",
    "pg": "^8.7.1",
    "redis": "^4.0.3",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.41"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "@types/node": "^16.11.19",
    "@types/pg": "^8.6.4",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.4.5",
    "jest-cli": "^27.4.5",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "supertest": "^6.1.6",
    "ts-jest": "^27.1.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  }
}
